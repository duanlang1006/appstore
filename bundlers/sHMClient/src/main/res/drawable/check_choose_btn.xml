<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
	<item android:state_checked="true" android:state_window_focused="false"
		android:state_enabled="true" android:drawable="@drawable/check_btn_2" />
	<item android:state_checked="false" android:state_window_focused="false"
		android:state_enabled="true" android:drawable="@drawable/check_btn_1" />
	<item android:state_checked="false" android:state_enabled="true"
		android:drawable="@drawable/check_btn_1" />
	<item android:state_checked="true" android:state_enabled="true"
		android:drawable="@drawable/check_btn_2" />

	<!-- Disabled states -->
	<item android:state_enabled="false" android:drawable="@drawable/check_btn_3" />


</selector>  